{
  "name": "ir-reattach",
  "version": "5.0.0",
  "main": "src/ir-reattach.js",
  "description": "The client code for the incremental renderer",
  "homepage": "https://github.com/donejs/ir-reattach",
  "repository": {
    "type": "git",
